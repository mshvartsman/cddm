isbn = {1553-7358},
issn = {15537358},
journal = {PLoS Computational Biology},
number = {5},
pmid = {24830758},
title = {{Structurally-Constrained Relationships between Cognitive States in the Human Brain}},
volume = {10},
year = {2014}
}
