keywords = {Attention,Cognition,Cognition: physiology,Eye Movements,Humans,Models,Psycholinguistics,Psychological,Reading},
month = jan,
number = {1},
pages = {125--57},
pmid = {9450374},
title = {{Toward a model of eye movement control in reading.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9450374},
volume = {105},
year = {1998}
}
