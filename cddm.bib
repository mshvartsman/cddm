keywords = {Attention,Auditory Perception,Cognition,Humans,Models,Problem Solving,Psychological,Psychological Theory,Psychomotor Performance,Reaction Time,Refractory Period,Visual Perception,bounded optimality,cognitive architecture,rational adaptation,response,theory comparison},
month = oct,
number = {4},
