pmid = {19839682},
publisher = {American Psychological Association},
title = {{Rational adaptation under task and processing constraints: implications for testing theories of cognition and action}},
url = {http://www-personal.umich.edu/~rickl/pubs/howes-lewis-vera-2009-psy-rev.pdf http://www.ncbi.nlm.nih.gov/pubmed/19839682},
