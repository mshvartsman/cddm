keywords = {Cognition,Cognition: physiology,Eye Movements,Eye Movements: physiology,Fixation,Humans,Ocular,Ocular: physiology,Reading,Visual Perception,Visual Perception: physiology},
month = feb,
number = {1},
pages = {1--56},
pmid = {16289074},
title = {{Tests of the E-Z Reader model: exploring the interface between cognition and eye-movement control.}},
url = {http://dx.doi.org/10.1016/j.cogpsych.2005.06.001},
volume = {52},
year = {2006}
}
