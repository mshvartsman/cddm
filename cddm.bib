journal = {Topics in cognitive science},
keywords = {cox proportional hazards model,eye movements,fixation duration,reading,time-},
month = jun,
pages = {1--23},
pmid = {23744668},
title = {{Proportional Hazards Modeling of Saccadic Response Times During Reading.}},
