issn = {0028-0836},
journal = {Nature},
keywords = {Adult,Brain Mapping,Cognition,Cognition: physiology,Female,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Memory,Prefrontal Cortex,Prefrontal Cortex: physiology,Psychomotor Performance,Short-Term,Short-Term: physiology},
