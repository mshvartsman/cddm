isbn = {1873-7838 (Electronic)$\backslash$n0010-0277 (Linking)},
issn = {00100277},
journal = {Cognition},
keywords = {Motivation,Response inhibition,Reward,Stop-signal response time,Stop-signal task},
number = {3},
