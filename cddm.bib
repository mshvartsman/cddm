journal = {Trends in cognitive sciences},
keywords = {Basal Ganglia,Basal Ganglia: physiology,Brain,Brain: physiology,Cerebral Cortex,Cerebral Cortex: physiology,Choice Behavior,Choice Behavior: physiology,Decision Making,Decision Making: physiology,Decision Support Techniques,Decision Theory,Humans,Nerve Net,Nerve Net: physiology},
