journal = {Psychological science},
keywords = {Choice Behavior,Choice Behavior: physiology,Discrimination (Psychology),Discrimination (Psychology): physiology,Humans,Judgment,Judgment: physiology,Memory,Memory Disorders,Memory Disorders: psychology,Noise,Normal Distribution,Perceptual Masking,Perceptual Masking: physiology,Short-Term,Short-Term: physiology,Task Performance and Analysis,Time Factors,Visual Perception,Visual Perception: physiology},
month = oct,
