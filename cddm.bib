month = nov,
number = {11},
pages = {1432--8},
pmid = {17057707},
title = {{Bayesian inference with probabilistic population codes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17057707},
volume = {9},
year = {2006}
}
