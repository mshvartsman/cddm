number = {2},
pages = {61--92},
pmid = {21699877},
publisher = {Elsevier Inc.},
title = {{Estimating the growth of internal evidence guiding perceptual decisions}},
url = {http://dx.doi.org/10.1016/j.cogpsych.2011.05.002},
